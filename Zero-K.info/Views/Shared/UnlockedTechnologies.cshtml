@model ZkData.Account
@foreach (var unlock in Model.AccountUnlocks.Select(x => x.Unlock)) {
  <span style="color: @unlock.LabelColor; font-size:x-small; display:inline-block; width:110px;height:60px" title="$unlock$@unlock.UnlockID"  >
    <img src="@unlock.ImageUrl"/><br />
    @unlock.Name
  </span>
}

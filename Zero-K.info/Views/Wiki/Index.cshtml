@model ZeroKWeb.Controllers.WikiController.WikiData
@{
    Page.Title =  Model.Node + " wiki";
    ViewBag.StaticBackground = true;
    ViewBag.OpaqueBody = true;
}
@Model.Content

@if (Model.Node == "LobbyStart") {
    <script type="text/javascript">
        $(document).ready(function () {
            _gaq.push(['_trackEvent', 'Lobby', 'First start']);
        });
    </script>
}
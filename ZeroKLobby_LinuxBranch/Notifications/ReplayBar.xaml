<UserControl x:Class="ZeroKLobby.Notifications.ReplayBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="389" Loaded="UserControl_Loaded">
	<DockPanel>
		<Button IsEnabled="False" DockPanel.Dock="Left" >Replay</Button>
		<Button IsEnabled="False" DockPanel.Dock="Right" Name="btnCancel" Click="btnCancel_Click">Cancel</Button>
		<Label Content="Label" HorizontalAlignment="Center" VerticalAlignment="Center" Name="label1"  DockPanel.Dock="Top" />
	</DockPanel>
	
	
</UserControl>
